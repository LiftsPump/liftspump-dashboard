{
  "family": "liftspump-dashboard",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::236783367292:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "liftspump-dashboard",
      "image": "123456789012.dkr.ecr.us-east-2.amazonaws.com/liftspump-dashboard:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/liftspump-dashboard",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "NEXT_PUBLIC_SUPABASE_URL",
          "value": "https://dupuztvhoifyczvqyjbk.supabase.co"
        },
        {
          "name": "NEXT_PUBLIC_SUPABASE_ANON_KEY",
          "value": "arn:aws:ssm:us-east-2:236783367292:parameter/NEXT_PUBLIC_SUPABASE_ANON_KEY"
        },
        {
          "name": "NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY",
          "value": "arn:aws:ssm:us-east-2:236783367292:parameter/NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY"
        }
      ],
      "secrets": [
        {
          "name": "STRIPE_SECRET_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:236783367292:secret:STRIPE_SECRET_KEY"
        },
        {
          "name": "STRIPE_WEBHOOK_SECRET",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:236783367292:secret:STRIPE_WEBHOOK_SECRET"
        },
        {
          "name": "SUPABASE_SERVICE_ROLE_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:236783367292:secret:SUPABASE_SERVICE_ROLE_KEY"
        }
      ]
    }
  ]
}
